<%= bootstrap_form_with model: @profile, local: false do |form| %>
  <%= form.text_field :firstname %>
  <%= form.text_field :lastname %><br>
  <%= form.file_field :image %>
  <%= form.select :country, options_for_select(CS.countries.map { |c| [c[1], c[0]] } ) %><br>
  <%= form.select :state, options_for_select([]) %><br>
  <%= form.select :city, options_for_select([]) %>

  <%= form.submit %>
<% end %>

